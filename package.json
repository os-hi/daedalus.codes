{
	"name": "daedalus.codes",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev:api": "node -e \"const exec = process.platform === 'win32' ? 'pocketbase.exe' : 'pocketbase'; require('child_process').execFileSync('./pb/' + exec, ['serve'], { stdio: 'inherit' })\"",
		"dev:web": "vite dev --host",
		"dev": "pnpm run dev:api & pnpm run dev:web",
		"dev:be": "./pb/pocketbase serve",
		"migrate": "./pb/pocketbase migrate",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check ./src && eslint ./src",
		"format": "prettier --write ./src",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@cartamd/plugin-attachment": "^3.0.2",
		"@cartamd/plugin-code": "^3.0.1",
		"@cartamd/plugin-emoji": "^3.1.0",
		"@cartamd/plugin-slash": "^3.1.0",
		"@iconify/svelte": "^3.1.4",
		"@playwright/test": "^1.28.1",
		"@skeletonlabs/skeleton": "^2.5.1",
		"@skeletonlabs/tw-plugin": "^0.3.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-cloudflare": "^3.0.0",
		"@sveltejs/adapter-node": "^2.0.0",
		"@sveltejs/enhanced-img": "^0.1.5",
		"@sveltejs/kit": "^2.0.0",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
		"@types/node": "^20.10.1",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.14",
		"carta-md": "^3.2.0",
		"clsx": "^2.0.0",
		"date-fns": "^2.30.0",
		"decode-formdata": "^0.4.0",
		"dotenv": "^16.3.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.30.0",
		"pocketbase": "^0.20.1",
		"postcss": "^8.4.24",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"sveltekit-search-params": "^1.0.16",
		"sveltekit-superforms": "^1.11.0",
		"sveltekit-view-transition": "^0.5.2",
		"tailwind-merge": "^2.0.0",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.0",
		"vite-plugin-tailwind-purgecss": "^0.1.4",
		"vitest": "^1.0.0",
		"zod": "^3.22.4",
		"@sveltejs/vite-plugin-svelte": "^3.0.0"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.5.3",
		"@fontsource-variable/inter": "^5.0.15",
		"embla-carousel-autoplay": "8.0.0-rc15",
		"embla-carousel-svelte": "8.0.0-rc15",
		"isomorphic-dompurify": "^1.12.0"
	}
}